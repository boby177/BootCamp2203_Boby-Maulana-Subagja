<!-- Content -->
<center> <H1> This is home page</H1> <br> </center>   
      <h2><p>Hello and Welcome <%= identity.name %> </p> </h2>
      <h3><%= identity.judul %> </h3>
      <br>
      

      <table class="table table-striped table-dark">
        <thead>
          <center><h3>Contact List JSON</h3></center>
        <% if (contacts.length < 1 ) { %> <!-- Terdapat hanya 3 data array, maka hasilnya akan empty -->
         <p>Contact is empty</p>
        <% } else { %>
          <tr>
            <th scope="col"> No </th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <!-- <th scope="col">Mobile</th> -->
          </tr>
        </thead>
        <tbody>
          <tr>
            <!-- <th scope="row">1</th> -->
            <% contacts.forEach((contacts, i) => { i++ %>
              <td><%= i %></td>
            <td><%= contacts.name %></td>
            <td> <%= contacts.email %></td>
            <br>
          </tr>
          <% })}; %>
        </tbody>
      </table>
    